{{ form_start(form)}}
     <div class="row">
       <div class="col-md-4">{{ form_row(form.firstName)}}</div>
       <div class="col-md-4">{{ form_row(form.lastName)}}</div>
       <div class="col-md-4">{{ form_row(form.cin)}}</div>
     </div>
     <div class="row">
       <div class="col-md-4">{{ form_row(form.birthDate)}}</div>
       <div class="col-md-4">{{ form_row(form.birthplace)}}</div>
       <div class="col-md-4">{{ form_row(form.sexe)}}</div>
     </div>
     <div class="row">
       <div class="col-md-4">{{ form_row(form.address)}}</div>
       <div class="col-md-4">{{ form_row(form.tel)}}</div>
       <div class="col-md-4">{{ form_row(form.email)}}</div>
     </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(form.type)}}</div>
    </div>
    {# <div class="row">
            <div class="col-md-4">{{ form_row(form.photo)}}</div>
      </div>#}
      
     <button class="btn">{{ button_label|default('Enregistrer') }}</button>
   {{ form_end(form)}}

 