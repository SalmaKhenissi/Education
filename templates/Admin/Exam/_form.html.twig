{{ form_start(form)}}
      
     <div class="row">
        <div class="col-md-4">{{ form_row(form.quarter)}}</div>
        <div class="col-md-4">{{ form_row(form.passAt)}}</div>
      </div>
      <div class="row">  
        <div class="col-md-4">{{ form_row(form.startAt)}}</div>
        <div class="col-md-4">{{ form_row(form.finishAt)}}</div>
     </div>
     <div class="row">
         <div class="col-md-4">{{ form_row(form.course)}}</div>
        <div class="col-md-4">{{ form_row(form.type)}}</div>
     </div>
     <button class="btn">{{ button_label|default('Suivant') }}</button>
   {{ form_end(form)}}