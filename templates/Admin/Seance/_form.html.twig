{{ form_start(form)}}
     <div class="row">
        <div class="col-md-4">{{ form_row(form.day)}}</div>
        
        <div class="col-md-4">{{ form_row(form.startAt)}}</div>
        <div class="col-md-4">{{ form_row(form.finishAt)}}</div>
     </div>
     <div class="row">
        <div class="col-md-4">{{ form_row(form.room)}}</div>
        <div class="col-md-4">{{ form_row(form.teacher)}}</div>
     </div>
     <button class="btn">{{ button_label|default('Enregistrer') }}</button>
   {{ form_end(form)}}